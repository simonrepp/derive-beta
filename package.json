{
  "author": "Simon Repp <simon@fdpl.io> (https://simonrepp.com)",
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "enolib": "^0.5.0",
    "enotype": "^0.3.0",
    "express": "^4.16.4",
    "fast-glob": "^2.2.6",
    "fs-extra": "^7.0.1",
    "markdown-it": "^8.4.2",
    "markdown-it-footnote": "^3.0.1",
    "mobx": "^5.9.4",
    "moment": "^2.24.0",
    "sharp": "^0.20.8",
    "speakingurl": "^14.0.1",
    "striptags": "^3.1.1"
  },
  "description": "dÃ©rive Atom Plugin Beta",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "fuse.js": "^3.4.4",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "turbolinks": "^5.2.0"
  },
  "engines": {
    "atom": ">=1.15.0 <2.0.0"
  },
  "license": "MIT",
  "main": "./lib/main",
  "name": "derive-beta",
  "package-deps": [
    "language-eno:0.4.2"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrepp/derive-beta.git"
  },
  "scripts": {
    "all-compile": "npm run derive-compile && npm run urbanize-compile",
    "derive-compile": "npm run derive-scripts && npm run derive-styles",
    "derive-scripts": "parcel build derive.at/scripts/main.js --experimental-scope-hoisting --no-cache --no-source-maps --out-dir derive.at/static --out-file scripts.js",
    "derive-styles": "node-sass --output-style compressed derive.at/styles/main.scss derive.at/static/styles.css",
    "urbanize-compile": "npm run urbanize-scripts && npm run urbanize-styles",
    "urbanize-scripts": "parcel build urbanize.at/scripts/main.js --experimental-scope-hoisting --no-cache --no-source-maps --out-dir urbanize.at/static --out-file scripts.js",
    "urbanize-styles": "node-sass --output-style compressed urbanize.at/styles/main.scss urbanize.at/static/styles.css"
  },
  "version": "1.22.12"
}
